; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Project Template"
run/main_scene="res://play_area/scenes/default_scene.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://textures/icon.svg"

[autoload]

EventBus="*res://play_area/scripts/event_bus/event_bus.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_state_charts/plugin.cfg")

[gui]

theme/custom="res://main_theme.tres"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
